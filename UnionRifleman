import greenfoot.*;
import java.util.List;

public class UnionRifleman extends UnionUnit
{
    public boolean isPlayer = true;
    public UnionRifleman() {
        super(100, 35, 2, 100, new GreenfootSound("RifleFire.mp3"));
    }
    
    public boolean getIsPlayer() {
        return isPlayer;
    }
}
