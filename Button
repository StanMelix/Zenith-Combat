import greenfoot.*; 
public class Button extends Actor
{
    private String unit;
    private int cost;
    public Button(String unit, int cost){
        GreenfootImage image = new GreenfootImage(60,60);
        image.setColor(new Color(255,0,0,127));
        image.fill();
        setImage(image);
        this.unit = unit;
        this.cost = cost;
    }
    public void act() 
    {
        // Add your action code here.
    }    
}
